{
	"name" : "laravel/laravel",
	"description" : "The Laravel Framework.",
	"keywords" : [
		"framework",
		"laravel"
	],
	"license" : "MIT",
	"type" : "project",
	"require" : {
		"php": "^7.1.3",
		"ext-mongodb": "*",
		"alfa6661/laravel-mongodb-cache": "^2.0",
		"aws/aws-sdk-php": "^3.81",
		"fideloper/proxy": "^4.0",
		"guzzlehttp/guzzle": "^6.3",
		"jenssegers/mongodb": "^3.4",
		"laravel/framework": "5.7.*",
		"laravel/socialite": "^3.2",
		"laravel/tinker": "^1.0",
		"maatwebsite/excel": "^3.1",
		"mongodb/mongodb": "^1.4",
		"oseintow/laravel-shopify": "^1.0",
		"phpclassic/php-shopify": "^1.0",
		"sentry/sentry-laravel": "~1"
	},
	"require-dev" : {
		"beyondcode/laravel-dump-server" : "^1.0",
		"filp/whoops" : "^2.0",
		"fzaninotto/faker" : "^1.4",
		"mockery/mockery" : "^1.0",
		"nunomaduro/collision" : "^2.0",
		"phpunit/phpunit" : "^7.0"
	},
	"autoload" : {
		"classmap" : [
			"database/seeds",
			"database/factories"
		],
		"psr-4" : {
			"App\\" : "app/"
		},
		"files" : [
			"app/Helper.php"
		]
	},
	"autoload-dev" : {
		"psr-4" : {
			"Tests\\" : "tests/"
		}
	},
	"extra" : {},
	"scripts" : {
		"post-root-package-install" : "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
		"post-create-project-cmd" : "@php artisan key:generate --ansi",
		"post-autoload-dump" : [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"@php artisan package:discover --ansi",
            "@php \"./storage/docs/phpDocumentor/phpDocumentor.phar\"",
            "@php \"./storage/docs/sami/sami.phar\" update \"./storage/docs/sami/sami.php\""
		]
	},
	"config" : {
		"preferred-install" : "dist",
		"sort-packages" : true,
		"optimize-autoloader" : true
	},
	"minimum-stability" : "dev",
	"prefer-stable" : true
}
